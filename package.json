{
    "name": "gitbook-plugin-cnzz",
    "version": "0.0.4",
    "description": "CNZZ Analytics tracking for your gitbook.",
    "repository": {
        "type": "git",
        "url": "https://github.com/image72/gitbook-plugin-cnzz"
    },
    "bugs": {
        "url": "https://github.com/image72/gitbook-plugin-cnzz/issues"
    },
    "readmeFilename": "README.md",
    "engines": {
        "gitbook": ">=3"
    },
    "gitbook": {
        "properties": {
            "visible": {
                "type": "boolean",
                "default": false,
                "description": "is the CNZZ analytics icon visible on site"
            },
            "style": {
                "type": "string",
                "default": "0",
                "description": "style for CNZZ analytics shown on site"
            },
            "siteid": {
                "type": "string",
                "required": true,
                "description": "you site id for CNZZ analytics"
            }
        }
    },
    "homepage": "https://github.com/image72/gitbook-plugin-cnzz",
    "license": "MIT",
    "keywords": [
        "gitbook",
        "plugin",
        "cnzz",
        "analytics"
    ]
}
